{
  "type": "module",
  "scripts": {
    "copy-env": "cp .env.example .env",
    "env-prod": "sed -i '' 's/^WP_ENVIRONMENT_TYPE=.*/WP_ENVIRONMENT_TYPE=production/' .env",
    "env-dev": "sed -i '' 's/^WP_ENVIRONMENT_TYPE=.*/WP_ENVIRONMENT_TYPE=development/' .env",
    "dev": "yarn env-dev && vite",
    "build": "yarn env-prod && vite build"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.3.4",
    "vite": "^4.4.9"
  },
  "dependencies": {
    "@vuelidate/core": "^2.0.2",
    "@vuelidate/validators": "^2.0.2",
    "axios": "^1.4.0",
    "jspdf": "^2.5.1",
    "stripe": "^12.8.0",
    "vue": "^3.3.4",
    "vue-router": "^4.2.0",
    "vue-select": "^4.0.0-beta.6",
    "vuex": "^4.0.2"
  }
}
